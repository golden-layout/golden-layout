{
  "name": "golden-layout",
  "version": "1.5.7",
  "author": "deepstreamHub GmbH",
  "license": "MIT",
  "devDependencies": {
    "grunt": "~1.0.1",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-gulp": "^1.0.1",
    "grunt-karma": "^2.0.0",
    "grunt-release": "~0.14.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-continuous-concat": "^0.1.1",
    "gulp-insert": "^0.5.0",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.5",
    "handlebars": "4.0.7",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "tslint": "^5.1.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "walker": "1.0.7"
  },
  "description": "A multi-screen javascript Layout manager \r https://golden-layout.com",
  "main": "./dist/goldenlayout.js",
  "typings": "./index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "jquery": "*"
  },
  "scripts": {
    "test": "grunt test",
    "tslint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\"",
    "prepublish": "typings install",
    "publish": "npm -s run tslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/golden-layout.git"
  },
  "keywords": [
    "layout manager",
    "javascript",
    "docker",
    "layout",
    "popouts"
  ],
  "bugs": {
    "url": "https://github.com/deepstreamIO/golden-layout/issues"
  },
  "homepage": "https://github.com/deepstreamIO/golden-layout",
  "npmName": "golden-layout",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "goldenlayout.js",
        "goldenlayout.min.js"
      ]
    },
    {
      "basePath": "/src/css/",
      "files": [
        "goldenlayout.base.css",
        "goldenlayout-dark-theme.css",
        "goldenlayout-light-theme.css"
      ]
    },
    {
      "basePath": "/",
      "files": [
        "typings.json"
      ]
    }
  ],
  "jspm": {
    "main": "dist/goldenlayout",
    "format": "global",
    "registry": "jspm",
    "dependencies": {
      "jquery": "^2.1.0"
    },
    "shim": {
      "dist/goldenlayout": [
        "jquery"
      ]
    }
  }
}
