{
  "name": "golden-layout",
  "version": "1.5.2",
  "author": "deepstreamHub GmbH",
  "license": "MIT",
  "devDependencies": {
    "grunt": "~0.4.2",
    "grunt-contrib-watch": "0.5.3",
    "grunt-release": "0.7.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.5.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "handlebars": "2.0.0-alpha.2",
    "karma": "0.12.16",
    "karma-chrome-launcher": "~0.1.4",
    "karma-coverage": "0.2.4",
    "karma-ie-launcher": "~0.1.5",
    "karma-jasmine": "~0.1.5",
    "tslint": "^3.7.3",
    "tslint-config-typings": "^0.2.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "walker": "1.0.6"
  },
  "description": "A multi-screen javascript Layout manager \r https://golden-layout.com",
  "main": "./dist/goldenlayout.js",
  "typings": "./index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "jquery": "*"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint \"**/*.ts\" -e \"node_modules/**\" -e \"typings/**\"",
    "prepublish": "typings install",
    "publish": "npm -s run tslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/golden-layout.git"
  },
  "keywords": [
    "layout manager",
    "javascript",
    "docker",
    "layout",
    "popouts"
  ],
  "bugs": {
    "url": "https://github.com/deepstreamIO/golden-layout/issues"
  },
  "homepage": "https://github.com/deepstreamIO/golden-layout",
  "npmName": "golden-layout",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "goldenlayout.js",
        "goldenlayout.min.js"
      ]
    },
    {
      "basePath": "/src/css/",
      "files": [
        "goldenlayout.base.css",
        "goldenlayout-dark-theme.css",
        "goldenlayout-light-theme.css"
      ]
    },
    {
      "basePath": "/",
      "files": [
        "typings.json"
      ]
    }
  ],
  "jspm": {
    "main": "dist/goldenlayout",
    "format": "global",
    "registry": "jspm",
    "dependencies": {
      "jquery": "^2.1.0"
    },
    "shim": {
      "dist/goldenlayout": [
        "jquery"
      ]
    }
  }
}
